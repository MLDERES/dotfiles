{
    "debug.inlineValues": true,
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'Input Mono', monospace",
    "editor.fontSize": 12,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderIndentGuides": true,
